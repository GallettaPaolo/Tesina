<h1>I tuoi dati</h1>
<div class="container">
    <div class="row">
        <div class="col s12 m3 l3">
            <img class="circle" src="<%=user.imgUrl%>" />
        </div>
        <div class="input-field col s12 m4 l4">
            <input value="<%=user.name%>" id="name" type="text" class="validate">
            <label class="active" for="name">Nome</label>
        </div>
        <div class="input-field col s12 m4 l4">
            <input value="<%=user.surname%>" id="surname" type="text" class="validate">
            <label for="surname" class="active">Cognome</label>
        </div>
    </div>
</div>